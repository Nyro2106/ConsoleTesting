<Window x:Class="WPFTesting.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFTesting"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Width="200" HorizontalAlignment="Left">
            <Button x:Name="UxOpenColorPickerButton" Content="Color Picker öffnen" Click="UxOpenColorPickerButton_Click"/>
            <Button x:Name="UxOpenLompfOMatButton" Content="Lompf-O-Mat öffnen" Click="UxOpenLompfOMatButton_Click"/>
            <Button x:Name="UxOpenShortCutsAndCommandsButton" Content="Shortcuts and Commands" Click="UxOpenShortCutsAndCommandsButton_Click"/>
        </StackPanel>
       

    </Grid>
</Window>
